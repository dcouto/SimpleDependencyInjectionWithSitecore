@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
        @Html.Sitecore().VisitorIdentification()
    </head>
    <body>
        <h1>@Html.Sitecore().Field("title")</h1>
        <div>
            @Html.Sitecore().Placeholder("main")
        </div>

        <script src="/includes/Project/MySolution/js/jquery-3.3.1.min.js"></script>

        <script src="/includes/Feature/GenericCallout/js/generic-callout.js"></script>
    </body>
</html>
